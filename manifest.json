{
  "name": "react-jquery-test",
  "short_name": "RJT",
  "version": "1.0.9",
  "manifest_version": 2,
  "description": "Testing React with jQuery",
  "homepage_url": "",
  "icons": {},
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
    },
    "default_title": "React jQuery Test"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab",
    "tabs",
    "webRequest",
    "storage"
  ],
  "options_ui": [],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/vendor.js",
        "scripts/common.js",
        "scripts/inject.js"
      ],
      "css": [
        "styles/inject.css"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    "fonts/*",
    "img/*",
    "styles/*"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}